#include<stdio.h>
#include<limits.h>
#include<float.h>

int main(void)
{
    printf("char max%c\n", CHAR_MAX);
    printf("short max%hd\n", SHRT_MAX);
    printf("double dig%d\n", DBL_DIG);
    printf("int max%d\n", INT_MAX);
    printf("int min%d\n", INT_MIN);
    printf("long int max%ld\n", LONG_MAX);
    printf("double max%lf\n", DBL_MAX);
    return 0;
}